<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="SelectedMenuBackground" Color="#050505"/>
    <SolidColorBrush x:Key="UnselectedListIconFill" Color="#050505"/>
    <SolidColorBrush x:Key="unselectedMenuForeground" Color="Gray"/>
    <SolidColorBrush x:Key="SelectedMenuForeground" Color="White"/>
    <SolidColorBrush x:Key="MenuHoverBrush" Color="#717987"/>
    
    <SolidColorBrush x:Key="PrimaryBackground" Color="#E8EDF5"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="White"/>
    <SolidColorBrush x:Key="PrimaryForeground" Color="Black"/>
    <SolidColorBrush x:Key="SecondaryForeground" Color="Gray"/>

</ResourceDictionary>